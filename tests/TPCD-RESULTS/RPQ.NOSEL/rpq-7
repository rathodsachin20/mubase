SELECT PSP1_SELKEY
FROM PARTSUBPART1, PARTSUBPART2, PARTSUBPART3, PARTSUBPART4,
    PARTSUBPART5, PARTSUBPART6, PARTSUBPART7
WHERE PSP1_SUBPARTKEY = PSP2_PARTKEY
AND PSP2_SUBPARTKEY = PSP3_PARTKEY
AND PSP3_SUBPARTKEY = PSP4_PARTKEY
AND PSP4_SUBPARTKEY = PSP5_PARTKEY
AND PSP5_SUBPARTKEY = PSP6_PARTKEY
AND PSP6_SUBPARTKEY = PSP7_PARTKEY;
