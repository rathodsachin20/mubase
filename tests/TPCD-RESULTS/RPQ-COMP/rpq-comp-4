SELECT PSP1_SELKEY
FROM PARTSUBPART1, PARTSUBPART2, PARTSUBPART3, PARTSUBPART4
WHERE PSP1_SELKEY < 10000 
AND PSP1_SUBPARTKEY = PSP2_PARTKEY
AND PSP2_SELKEY < 10000
AND PSP2_SUBPARTKEY = PSP3_PARTKEY
AND PSP3_SELKEY < 10000
AND PSP3_SUBPARTKEY = PSP4_PARTKEY
AND PSP4_SELKEY < 10000;

SELECT PSP1_SELKEY
FROM PARTSUBPART1, PARTSUBPART2, PARTSUBPART3
WHERE PSP1_SELKEY < 10000 
AND PSP1_SUBPARTKEY = PSP2_PARTKEY
AND PSP2_SELKEY < 10000
AND PSP2_SUBPARTKEY = PSP3_PARTKEY
AND PSP3_SELKEY < 10000;

SELECT PSP1_SELKEY
FROM PARTSUBPART1, PARTSUBPART2
WHERE PSP1_SELKEY < 10000 
AND PSP1_SUBPARTKEY = PSP2_PARTKEY
AND PSP2_SELKEY < 10000;

SELECT PSP1_SELKEY
FROM PARTSUBPART1
WHERE PSP1_SELKEY < 100000;
