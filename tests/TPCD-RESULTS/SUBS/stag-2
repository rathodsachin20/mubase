SELECT MAX(PSP0_SELKEY)
FROM PARTSUBPART0, PARTSUBPART2, PARTSUBPART4, PARTSUBPART6, PARTSUBPART8
WHERE PSP0_SELKEY < 50
AND PSP0_SUBPARTKEY = PSP2_PARTKEY
AND PSP2_SELKEY < 50
AND PSP2_SUBPARTKEY = PSP4_PARTKEY
AND PSP4_SELKEY < 50
AND PSP4_SUBPARTKEY = PSP6_PARTKEY
AND PSP6_SELKEY < 50
AND PSP6_SUBPARTKEY = PSP8_PARTKEY
AND PSP8_SELKEY < 50
GROUP BY PSP0_SELKEY;

SELECT MAX(PSP0_SELKEY)
FROM PARTSUBPART0, PARTSUBPART2, PARTSUBPART4, PARTSUBPART6, PARTSUBPART9
WHERE PSP0_SELKEY < 60
AND PSP0_SUBPARTKEY = PSP2_PARTKEY
AND PSP2_SELKEY < 60
AND PSP2_SUBPARTKEY = PSP4_PARTKEY
AND PSP4_SELKEY < 60
AND PSP4_SUBPARTKEY = PSP6_PARTKEY
AND PSP6_SELKEY < 60
AND PSP6_SUBPARTKEY = PSP9_PARTKEY
AND PSP9_SELKEY < 60
GROUP BY PSP0_SELKEY;

