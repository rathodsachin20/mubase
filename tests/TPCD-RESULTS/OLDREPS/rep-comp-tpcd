SELECT MIN(CUSTOMER.C_CUSTKEY)
FROM CUSTOMER, ORDERS, LINEITEM, NATION
WHERE CUSTOMER.C_CUSTKEY = ORDERS.O_CUSTKEY
  AND LINEITEM.L_ORDERKEY = ORDERS.O_ORDERKEY
  AND ORDERS.O_SELKEY = 1  
