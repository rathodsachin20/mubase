SELECT P_SELKEY
FROM PART, SUPPLIER, LINEITEM, PARTSUPP, ORDERS, NATION
WHERE S_SUPPKEY  = L_SUPPKEY
AND PS_SUPPKEY = L_SUPPKEY
AND PS_PARTKEY = L_PARTKEY
AND P_PARTKEY  = L_PARTKEY
AND O_ORDERKEY = L_ORDERKEY
AND S_NATIONKEY   = N_NATIONKEY
AND P_SELKEY > 251 AND P_SELKEY < 260;

SELECT P_SELKEY
FROM PART, SUPPLIER, LINEITEM, PARTSUPP, ORDERS, NATION
WHERE S_SUPPKEY  = L_SUPPKEY
AND PS_SUPPKEY = L_SUPPKEY
AND PS_PARTKEY = L_PARTKEY
AND P_PARTKEY  = L_PARTKEY
AND O_ORDERKEY = L_ORDERKEY
AND S_NATIONKEY   = N_NATIONKEY
AND P_SELKEY > 241 AND P_SELKEY < 250;

SELECT S_SUPPKEY
FROM SUPPLIER, LINEITEM, ORDERS, CUSTOMER, NATION, NATION1
WHERE S_SUPPKEY  = L_SUPPKEY
AND O_ORDERKEY = L_ORDERKEY
AND C_CUSTKEY  = O_CUSTKEY
AND S_NATIONKEY   = NATION.N_NATIONKEY
AND C_NATIONKEY   = NATION1.N1_NATIONKEY
AND ((NATION.N_NATIONKEY = 1 AND NATION1.N1_NATIONKEY = 2)
 OR (NATION.N_NATIONKEY = 2 AND NATION1.N1_NATIONKEY = 1))
AND L_SELKEY > 16;

SELECT S_SUPPKEY
FROM SUPPLIER, LINEITEM, ORDERS, CUSTOMER, NATION, NATION1
WHERE S_SUPPKEY  = L_SUPPKEY
AND O_ORDERKEY = L_ORDERKEY
AND C_CUSTKEY  = O_CUSTKEY
AND S_NATIONKEY   = NATION.N_NATIONKEY
AND C_NATIONKEY   = NATION1.N1_NATIONKEY
AND ((NATION.N_NATIONKEY = 4 AND NATION1.N1_NATIONKEY = 3)
 OR (NATION.N_NATIONKEY = 3 AND NATION1.N1_NATIONKEY = 4))
AND L_SELKEY > 16;

SELECT MAX(O_SELKEY)
FROM CUSTOMER, ORDERS, LINEITEM, SUPPLIER, NATION, REGION
WHERE C_CUSTKEY = O_CUSTKEY
  AND O_ORDERKEY = L_ORDERKEY
  AND L_SUPPKEY = S_SUPPKEY
  AND C_NATIONKEY = S_NATIONKEY
  AND S_NATIONKEY = N_NATIONKEY
  AND N_REGIONKEY = R_REGIONKEY
  AND R_REGIONKEY = 1
  AND O_SELKEY < 5
GROUP BY N_NATIONKEY;

SELECT MAX(O_SELKEY)
FROM CUSTOMER, ORDERS, LINEITEM, SUPPLIER, NATION, REGION
WHERE C_CUSTKEY = O_CUSTKEY
  AND O_ORDERKEY = L_ORDERKEY
  AND L_SUPPKEY = S_SUPPKEY
  AND C_NATIONKEY = S_NATIONKEY
  AND S_NATIONKEY = N_NATIONKEY
  AND N_REGIONKEY = R_REGIONKEY
  AND R_REGIONKEY = 2
  AND O_SELKEY < 5
GROUP BY N_NATIONKEY;

SELECT O_SELKEY
FROM CUSTOMER, ORDERS, LINEITEM
WHERE C_SELKEY  = 1
AND C_CUSTKEY    = O_CUSTKEY
AND L_ORDERKEY   = O_ORDERKEY
AND O_SELKEY  < 13
AND L_SELKEY  > 12;

SELECT O_SELKEY
FROM CUSTOMER, ORDERS, LINEITEM
WHERE C_SELKEY  = 2
AND C_CUSTKEY    = O_CUSTKEY
AND L_ORDERKEY   = O_ORDERKEY
AND O_SELKEY  < 13
AND L_SELKEY  > 12;

SELECT MIN(CUSTOMER.C_SELKEY)
FROM CUSTOMER, ORDERS, LINEITEM, NATION
WHERE CUSTOMER.C_CUSTKEY = ORDERS.O_CUSTKEY
  AND LINEITEM.L_ORDERKEY = ORDERS.O_ORDERKEY
  AND ORDERS.O_SELKEY = 1  
  AND LINEITEM.L_SELKEY < 7
  AND CUSTOMER.C_NATIONKEY = NATION.N_NATIONKEY
GROUP BY CUSTOMER.C_CUSTKEY, NATION.N_NATIONKEY;

SELECT MIN(CUSTOMER.C_SELKEY)
FROM CUSTOMER, ORDERS, LINEITEM, NATION
WHERE CUSTOMER.C_CUSTKEY = ORDERS.O_CUSTKEY
  AND LINEITEM.L_ORDERKEY = ORDERS.O_ORDERKEY
  AND ORDERS.O_SELKEY = 2  
  AND LINEITEM.L_SELKEY < 7
  AND CUSTOMER.C_NATIONKEY = NATION.N_NATIONKEY
GROUP BY CUSTOMER.C_CUSTKEY, NATION.N_NATIONKEY;

