SELECT S_SUPPKEY
FROM SUPPLIER, LINEITEM, CUSTOMER, NATION, REGION, NATION1, ORDERS
WHERE S_SUPPKEY   =     L_SUPPKEY
    AND  S_SUPPKEY  =     NATION1.N1_NATIONKEY
    AND  S_SUPPKEY  =     O_ORDERKEY
    AND  S_SUPPKEY   =     C_CUSTKEY
    AND  S_SUPPKEY    =  NATION.N_NATIONKEY
    AND  S_SUPPKEY =  R_REGIONKEY;

